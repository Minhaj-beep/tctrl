{
  "expo": {
    "name": "ctrl",
    "slug": "ctrl",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/ctrl.png",
      "resizeMode": "contain",
      "backgroundColor": "#081537"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ctrl.png",
        "backgroundColor": "#081537"
      },
      "package": "com.thoughtctrl.ctrl"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
